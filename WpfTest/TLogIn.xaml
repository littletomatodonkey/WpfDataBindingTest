<Window x:Class="WpfTest.DataBinding.TLogIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TLogIn" Height="300" Width="300" Closing="Window_Closing">
    <Grid>
        <Button Name="btnLogin" Content="Sure Log In?" HorizontalAlignment="Left" Margin="81,111,0,0" VerticalAlignment="Top" Width="126" Click="btnLogin_Click"/>
        <TextBox Name="tbLogin" HorizontalAlignment="Left" Height="23" Margin="81,70,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button Content="Cancel" Name="btnCancel" HorizontalAlignment="Left" Margin="81,152,0,0" VerticalAlignment="Top" Width="75" Click="btnCancel_Click"/>
        

    </Grid>
</Window>
